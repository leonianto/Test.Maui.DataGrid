<?xml version="1.0" encoding="utf-8" ?>
<Stepper xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Maui.DataGrid.CustomStepper">

    <Stepper.Style>
        <Style TargetType="Stepper">
            <Setter Property="Margin" Value="5"></Setter>
            <Setter Property="BackgroundColor" Value="{AppThemeBinding Light={StaticResource PapayaWhip}, Dark={StaticResource DarkPapayaWhip}}"></Setter>
        </Style>
    </Stepper.Style>
    
</Stepper>
